<view class="container">
  <image src="{{banner.img}}" class="title" mode="widthFix" bind:tap="onMoreTap"></image>
  <view class="inner-container">
    <view >
      <image class="left" src="{{left.img}}" bind:tap="onItemTap" data-banner_id="{{left.banner_id}}"></image>
    </view>

    <view class="right-container">
      <image class="right-size" src="{{rightTop.img}}" bind:tap="onItemTap" data-banner_id="{{rightTop.banner_id}}"></image>
      <image class="right-size" src="{{rightBottom.img}}" bind:tap="onItemTap" data-banner_id="{{rightBottom.banner_id}}"></image>
    </view>

  </view>
</view>