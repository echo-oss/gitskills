<view class="container">
  <image src="{{banner.img}}" class="title" mode="widthFix" bind:tap="onMoreTap"></image>
  <view class="inner-container">
    <view hover-class="rect-hover" hover-stay-time="300">
      <image class="left" src="{{left.img}}" bind:tap="onItemTap" data-banner_id="{{left.id}}"></image>
    </view>

    <view class="right-container" >
      <image  class="right-size" src="{{rightTop.img}}" bind:tap="onItemTap" data-banner_id="{{rightTop.id}}"></image>
      <image class="right-size" src="{{rightBottom.img}}" bind:tap="onItemTap" data-banner_id="{{rightBottom.id}}"></image>
    </view>

  </view>
</view>